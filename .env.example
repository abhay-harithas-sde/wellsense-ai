# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORG_ID=your_organization_id_here

# Google AI Configuration
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# Anthropic Claude Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Cohere Configuration
COHERE_API_KEY=your_cohere_api_key_here

# Hugging Face Configuration
HUGGING_FACE_TOKEN=your_hugging_face_token_here

# Server Configuration
PORT=5000
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# Database Configuration
# Primary database (mongodb, mysql, postgresql, sqlite)
PRIMARY_DATABASE=mongodb

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/wellsense-ai

# MySQL Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DATABASE=wellsense_ai

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_DATABASE=wellsense_ai

# SQLite Configuration
SQLITE_PATH=./data/wellsense-ai.db
ENABLE_SQLITE=false

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# File Upload Configuration
MAX_FILE_SIZE=10mb
UPLOAD_PATH=./uploads

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# AI Configuration
AI_MODEL_PRIMARY=gpt-4
AI_MODEL_SECONDARY=gpt-3.5-turbo
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=1000

# AI Provider Priority (comma-separated)
AI_PROVIDER_PRIORITY=openai-gpt4,anthropic-claude,google-gemini,openai-gpt35,cohere,huggingface,fallback

# LLM Service Configuration
LLM_DEFAULT_PROVIDER=openai
LLM_FALLBACK_ENABLED=true
LLM_MAX_TOKENS=1000
LLM_TEMPERATURE=0.7

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT=your_deployment_name

# Ollama Configuration (Local LLM)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# LLM Rate Limiting
LLM_RATE_LIMIT_ENABLED=true
LLM_MAX_REQUESTS_PER_HOUR=100
LLM_COST_TRACKING_ENABLED=true