# API Configuration
# Leave empty to use relative paths (recommended for Vercel)
VITE_API_URL=

# Video Meeting URL
# Default: https://meet.wellsense.in
VITE_MEETING_URL=https://meet.wellsense.in

# App URL (optional - auto-detected from window.location.origin)
VITE_APP_URL=

# OpenAI API Key (backend only)
OPENAI_API_KEY=your_openai_api_key_here

# Database URLs
DATABASE_URL=your_postgresql_url
MONGODB_URI=your_mongodb_url
REDIS_URL=your_redis_url

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://your-domain.vercel.app/auth/callback

# Microsoft OAuth
MICROSOFT_CLIENT_ID=your_microsoft_client_id
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret
MICROSOFT_REDIRECT_URI=https://your-domain.vercel.app/auth/callback

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Environment
NODE_ENV=production
